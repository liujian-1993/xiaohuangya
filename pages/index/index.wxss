.faq {
    width: 745rpx;
    margin: 1rpx auto 0;
}

.faq-header {
    display: flex;
    justify-content: space-between;
    margin-bottom: 1rpx;
}

.faq-header-title {
    font-size: 24rpx;
    font-weight: 600;
}

.faq-header-more {
    font-size: 24rpx;
    color: #000;
}

.faq-content {
    width: 100%;
    border-radius: 12rpx;
    background: #0000;
}

.faq-content-list {
    overflow: hidden;
    height: 44rpx;
    line-height: 44rpx;
}

.faq-content-list text {
    font-size: 21rpx;
    color: #666;
    margin-left: 10rpx;
}

