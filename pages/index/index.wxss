.watermark {
    position: relative;
    background-color: #fff;
    border-top-left-radius: 32rpx;
    border-top-right-radius: 32rpx;
    padding-top: 100rpx;
    top: 565rpx;
    z-index: 9;
    /* border: 1px red solid */
}

.watermark-input {
    position: absolute;
    top: -110rpx;
    left: 48%;
    width: 700rpx;
    height: 80rpx;
    margin-left: -332rpx;
    z-index: 9;
    background-color: #0000;
    box-shadow: 0 14rpx 28rpx hsla(0,11%,15%,0.3);
    border-radius: 32rpx;
}

.watermark-input input {
    float: left;
    width: 80%;
    height: 100%;
    font-size: 34rpx;
    padding-left: 40rpx;
}

#clearInputText {
    display: flex;
    width: 100rpx;
    height: 100%;
    border: none;
    background-color: #fff;
    align-items: center;
}

#clearInputText:after {
    border: none;
}

#clearInputText image {
    display: block;
    width: 50rpx;
    height: 50rpx;
}

.parsing {
    overflow: hidden;
    display: block;
    width: 264rpx;
    height: 100rpx;
    line-height: 100rpx;
    color: #fff;
    background-color: #e8453c;
    box-shadow: 0 14rpx 28rpx hsla(0,11%,15%,0.4);
    margin-top: -30rpx;
}

